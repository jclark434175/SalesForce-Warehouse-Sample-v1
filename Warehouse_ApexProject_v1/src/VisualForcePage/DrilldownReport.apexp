                     <apex:inputHidden id="usrMplan" value="{!currMailPlan}"/>
                     <apex:inputHidden id="usrTitl" value="{!currTitle}"/>
                     <apex:inputHidden id="zoneFilter" value="{!zoneFilter}"/>
                     <table id="filter_header" width="950" border="0" cellpadding="0" cellspacing="0">
                        <tr>
                            <td height="10">
                            </td>
                        </tr>
                        <tr align="right">
                            <td align="right">
                                <table border="0" cellspacing="0" cellpadding="0">
                                  <tr align="right">
                                    <td>
