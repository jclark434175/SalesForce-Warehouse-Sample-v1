                                    -->
                                    (  
                                    <apex:outputPanel rendered="{!AND(rptCfg.Report_Type__c!='Downline_Detail',
                                                                      rptCfg.Report_Type__c!='Birthday_Anniversaries',
                                                                      LEN(recCnt)>0
                                                                      )}">
                                    {!$Label.Total}: {!recCnt} / 
                                    </apex:outputPanel>
                                    {!$Label.Displaying} 
                                    <apex:outputPanel rendered="{!reportRows.size>0}">
                                        <apex:outputText value="{!((pageNbr-1)*100)+1}"/> - <apex:outputText value="{!((pageNbr-1)*100)+reportRows.size}"/>
                                    </apex:outputPanel>
                                    <apex:outputText value="0" rendered="{!reportRows.size=0}"/> 
                                    <apex:outputText value="{!$Label.This_Generation}" rendered="{!userProfile='Avon Leader'}"/>
                                    <apex:outputText value="{!$Label.This_Title}" rendered="{!OR(userProfile='Avon Zone Manager',userProfile='Avon Division Manager')}"/> )
                                    </td>
                                    <td width="3"><img src="{!URLFOR($Resource.AvonSiteAssets,'images/table_right_edge.gif')}"/></td>
                                </tr>
                            </table>
                            </apex:outputPanel>
                        </td>
                        </tr>
                    </table>
                    <div id="processing_msg" style="position:absolute;left:0px;top:0px;z-index:4;background-color:#FFFFFF;opacity:.90;filter:Alpha(opacity=90);">
                        <div id="inner_msg_table" style="position:relative;vertical-align:middle;top:30%">
                          <table width="100%" border="0" cellpadding="0" cellspacing="1">
                            <tr align="center" >
                              <td><img src="{!URLFOR($Resource.AvonSiteAssets,'images/processing.gif')}"></td>
                            </tr>
                            <tr align="center" >
                              <td>{!$Label.Processing_Status_Message}</td>
                            </tr>
                          </table>
                      </div>
                    </div>
                <script type="text/javascript">
                    showHideMessage('hidden');
                </script>
                    
                    <table width="950" border="0" cellpadding="0" cellspacing="0">
                        <tr>
                            <td width="5"> </td>
                            <td>
                            <apex:outputPanel rendered="{!NOT(zoneSelErr)}">
                            <table width="950" border="0" cellpadding="0" cellspacing="0">
                                <tr class="information_header">                   
                                <td  width="20">  </td>
